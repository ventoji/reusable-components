  "scripts": {
    "prestart": "npm run gen:docs",
    "start": "npm-run-all --parallel start:docs gen:docs-watch",
    "start:docs": "node scripts/start.js",
    "gen:docs": "node scripts/generateComponentData.js",
    "gen:docs-watch": "npm run gen:docs -- --watch",
    "build:custom": "cross-env NODE_ENV=production webpack --config ./config/webpack.lib.js ",
    "build:docs": "node scripts/build.js",
    "test:local": "node scripts/test.js -env=jsdom",
    "build:css": "cpx \"./src/components/**/*.css\" ./lib",
    "prebuild:lib": "rimraf lib",
    "build:lib": "npm-run-all --parallel build:commonjs build:css build:copy-files",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "predeploy:docs": "npm run build:docs",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore ./src/**/*.spec.js,src/**/*.test.js",
    "commonjs": "cross-env NODE_ENV=production babel --plugins @babel/plugin-transform-modules-commonjs src/components/PasswordInput/PasswordInput.js",
    "prepublish": "npm run build:lib",
    "ncu": "ncu",
    "ncu:update": "ncu -u",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "release": "standard-version",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test:coverage": "npm run test:local -- --coverage --collectCoverageFrom --watchAll=false",
    "test": "jest --ci --coverage --collectCoverageFrom && codecov",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
